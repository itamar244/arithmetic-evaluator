{
		"parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
        "import",
				"flowtype"
    ],
		"rules": {
			"semi": ["error", "never"],
			"indent": [2, "tab", { "SwitchCase": 1 }],
			"no-tabs": [0],
			"no-use-before-define": [0],
			"no-nested-ternary": [0],
			"no-mixed-operators": [0]
		}
}
